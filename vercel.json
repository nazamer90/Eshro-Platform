{
  "version": 2,
  "name": "eshro-platform",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install --force",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./frontend/",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}